(test-begin "bags/lowlevel")
(define bucket (bag string-ci-comparator "abc" "def"))
(test-equal string-ci-comparator (bag-element-comparator bucket))
(test-assert (bag-contains? bucket "abc"))
(test-assert (bag-contains? bucket "ABC"))
(test-equal "def" (bag-member bucket "DEF" "fqz"))
(test-equal "fqz" (bag-member bucket "lmn" "fqz"))
(let* ((nums (bag number-comparator 1 2 3))
       ;; nums is now {1, 2, 3}
       (nums2 (bag-replace nums 2.0))
       (bob (bag bag-comparator
                 (bag eqv-comparator 1 2)
                 (bag eqv-comparator 1 2))))
  ;; nums2 is now {1, 2.0, 3}
  (test-assert (bag-any? inexact? nums2))
  (bag-replace! nums 2.0)
  ;; nums is now {1, 2.0, 3}
  (test-assert (bag-any? inexact? nums))
  (test-equal 2 (bag-size bob)))
(test-end "bags/lowlevel")


(test-begin "bags/semantics")
(define mybag (bag number-comparator 1 2))
;; mybag is {1, 2}
(test-equal 2 (bag-size mybag))
(bag-adjoin! mybag 1)
;; mybag is {1, 1, 2}
(test-equal 3 (bag-size mybag))
(test-equal 2 (bag-unique-size mybag))
(bag-delete! mybag 2)
;; mybag is {1, 1}
(bag-delete! mybag 2)
(test-equal 2 (bag-size mybag))
(bag-increment! mybag 1 3)
;; mybag is {1, 1, 1, 1, 1}
(test-equal 5 (bag-size mybag))
(test-assert (bag-decrement! mybag 1 2))
;; mybag is {1, 1, 1}
(test-equal 3 (bag-size mybag))
(bag-decrement! mybag 1 5)
;; mybag is {}
(test-equal 0 (bag-size mybag))
(test-end "bags/semantics")

(test-begin "bags/convert")
(define multi (bag eqv-comparator 1 2 2 3 3 3))
(define single (bag eqv-comparator 1 2 3))
(define singleset (set eqv-comparator 1 2 3))
(define minibag (bag eqv-comparator 'a 'a))
(define alist '((a . 2)))
(test-equal alist (bag->alist minibag))
(test-assert (bag=? minibag (alist->bag eqv-comparator alist)))
(test-assert (set=? singleset (bag->set single)))
(test-assert (set=? singleset (bag->set multi)))
(test-assert (bag=? single (set->bag singleset)))
(test-assert (not (bag=? multi (set->bag singleset))))
(set->bag! minibag singleset)
;; minibag is now {a, a, a, a, 1, 2, 3}
(test-assert (bag-contains? minibag 1))
(test-end "bags/convert")

(test-begin "bags/sumprod")
(let* ((abb (bag eq-comparator 'a 'b 'b))
       (aab (bag eq-comparator 'a 'a 'b))
       (total (bag-sum abb aab))
       (bag1 (bag eqv-comparator 1)))
  (test-equal 3 (bag-count (lambda (x) (eqv? x 'a)) total))
  (test-equal 3 (bag-count (lambda (x) (eqv? x 'b)) total))
  (test-equal 12 (bag-size (bag-product 2 total)))
  (bag-sum! bag1 bag1)
  (test-equal 2 (bag-size bag1))
  (bag-product! 2 bag1)
  (test-equal 4 (bag-size bag1)))
(test-end "bags/sumprod")

