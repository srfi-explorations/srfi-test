(test-begin "sets/ops")
;; Potentially mutable
(let ((abcd (set eq-comparator 'a 'b 'c 'd))
      (efgh (set eq-comparator 'e 'f 'g 'h))
      (abgh (set eq-comparator 'a 'b 'g 'h))
      ;; Never get a chance to be mutated
      (other-abcd (set eq-comparator 'a 'b 'c 'd))
      (other-efgh (set eq-comparator 'e 'f 'g 'h))
      (other-abgh (set eq-comparator 'a 'b 'g 'h))
      (all (set eq-comparator 'a 'b 'c 'd 'e 'f 'g 'h))
      (none (set eq-comparator))
      (ab (set eq-comparator 'a 'b))
      (cd (set eq-comparator 'c 'd))
      (ef (set eq-comparator 'e 'f))
      (gh (set eq-comparator 'g 'h))
      (cdgh (set eq-comparator 'c 'd 'g 'h))
      (abcdgh (set eq-comparator 'a 'b 'c 'd 'g 'h))
      (abefgh (set eq-comparator 'a 'b 'e 'f 'g 'h)))
  (test-assert (set-disjoint? abcd efgh))
  (test-assert (not (set-disjoint? abcd ab)))
  ;; FIXME
  #;(parameterize ((current-test-comparator set=?))
  (test-equal abcd (set-union abcd))
  (test-equal all (set-union abcd efgh))
  (test-equal abcdgh (set-union abcd abgh))
  (test-equal abefgh (set-union efgh abgh))
  (define efgh2 (set-copy efgh))
  (set-union! efgh2)
  (test-equal efgh efgh2)
  (set-union! efgh2 abgh)
  (test-equal abefgh efgh2)
  (test-equal abcd (set-intersection abcd))
  (test-equal none (set-intersection abcd efgh))
  (define abcd2 (set-copy abcd))
  (set-intersection! abcd2)
  (test-equal abcd abcd2)
  (set-intersection! abcd2 efgh)
  (test-equal none abcd2)
  (test-equal ab (set-intersection abcd abgh))
  (test-equal ab (set-intersection abgh abcd))
  (test-equal abcd (set-difference abcd))
  (test-equal cd (set-difference abcd ab))
  (test-equal abcd (set-difference abcd gh))
  (test-equal none (set-difference abcd abcd))
  (define abcd3 (set-copy abcd))
  (set-difference! abcd3)
  (test-equal abcd abcd3)
  (set-difference! abcd3 abcd)
  (test-equal none abcd3)
  (test-equal cdgh (set-xor abcd abgh))
  (test-equal all (set-xor abcd efgh))
  (test-equal none (set-xor abcd other-abcd))
  (define abcd4 (set-copy abcd))
  ;; don't test xor! effect
  (test-equal none (set-xor! abcd4 other-abcd))
  (test-equal "abcd smashed?" other-abcd abcd)
  (test-equal "efgh smashed?" other-efgh efgh)
  (test-equal "abgh smashed?" other-abgh abgh))
)
(test-end "sets/ops")

