(test-begin "sets/simple")
(define nums (set number-comparator))
;; nums is now {}
(define syms (set eq-comparator 'a 'b 'c 'd))
;; syms is now {a, b, c, d}
(define nums2 (set-copy nums))
;; nums2 is now {}
(define syms2 (set-copy syms))
;; syms2 is now {a, b, c, d}
(define esyms (set eq-comparator))
;; esyms is now {}
(test-assert (set-empty? esyms))
(define total 0)
(test-assert (set? nums))
(test-assert (set? syms))
(test-assert (set? nums2))
(test-assert (set? syms2))
(test-assert (not (set? 'a)))
(set-adjoin! nums 2)
(set-adjoin! nums 3)
(set-adjoin! nums 4)
(set-adjoin! nums 4)
;; nums is now {2, 3, 4}
(test-equal 4 (set-size (set-adjoin nums 5)))
(test-equal 3 (set-size nums))
(test-equal 3 (set-size (set-delete syms 'd)))
(test-equal 2 (set-size (set-delete-all syms '(c d))))
(test-equal 4 (set-size syms))
(set-adjoin! syms 'e 'f)
;; syms is now {a, b, c, d, e, f}
(test-equal 4 (set-size (set-delete-all! syms '(e f))))
;; syms is now {a, b, c, d}
(test-equal 0 (set-size nums2))
(test-equal 4 (set-size syms2))
(set-delete! nums 2)
;; nums is now {3, 4}
(test-equal 2 (set-size nums))
(set-delete! nums 1)
(test-equal 2 (set-size nums))
(set! nums2 (set-map number-comparator (lambda (x) (* 10 x)) nums))
;; nums2 is now {30, 40}
(test-assert (set-contains? nums2 30))
(test-assert (not (set-contains? nums2 3)))
(set-for-each (lambda (x) (set! total (+ total x))) nums2)
(test-equal 70 total)
(test-equal 10 (set-fold + 3 nums))
(set! nums (set eqv-comparator 10 20 30 40 50))
;; nums is now {10, 20, 30, 40, 50}
(test-assert
  (set=? nums (set-unfold
                (lambda (i) (= i 0))
                (lambda (i) (* i 10))
                (lambda (i) (- i 1))
                5
                eqv-comparator)))
(test-equal '(a) (set->list (set eq-comparator 'a)))
(set! syms2 (list->set eq-comparator '(e f)))
;; syms2 is now {e, f}
(test-equal 2 (set-size syms2))
(test-assert (set-contains? syms2 'e))
(test-assert (set-contains? syms2 'f))
(list->set! syms2 '(a b))
(test-equal 4 (set-size syms2))
(test-end "sets/simple")

